local Tab = Window:NewTab("VIP SERVER")



local Section = Tab:NewSection("Can Have Bugs")


Section:NewDropdown("Item Summon", "", {"Ice Core", "Forstbitten Souls", "Ice Fragment", "Burning Present"}, function(s)
    if s == "Ice Core" then
for i , obj in pairs(game.Workspace:GetDescendants()) do
if obj.Name == "Ice Core" then
obj.CFrame = player.Character.HumanoidRootPart.CFrame
end
end
    elseif s == "Forstbitten Souls" then
local player = game.Players.LocalPlayer
for i , obj in pairs(game.Workspace:GetDescendants()) do
if obj.Name == "Frostbitten Soul" then
obj.CFrame = player.Character.HumanoidRootPart.CFrame
end
end

    elseif s == "Ice Fragment" then
local player = game.Players.LocalPlayer
for i , obj in pairs(game.Workspace:GetDescendants()) do
if obj.Name == "Ice Fragment" then
obj.CFrame = player.Character.HumanoidRootPart.CFrame
end
end
    end
end)





